{
    "name": "getsky/phalcon-config-loader",
    "type": "library",
    "description": "Component to load configuration from various files",
    "keywords": [ "framework", "phalcon", "config"],
    "license": "MIT",
    "authors": [
        {
            "name": "JimmDiGrizli",
            "email": "getmansk-y@yandex.ru"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-phalcon": ">=1.2.4,<2",
        "symfony/yaml": "master"
    },
    "require-dev": {
        "phalcon/devtools": "dev-master",
        "phpunit/phpunit": "3.7.*",
        "squizlabs/php_codesniffer": "1.*",
        "codeception/codeception": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "GetSky\\Phalcon\\ConfigLoader\\": "src/"
        }
    }
}
